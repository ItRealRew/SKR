<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<managed-bean>
        <managed-bean-name>MainBean</managed-bean-name>
        <managed-bean-class>BeansOld.MainBean</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/UStorage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toUProduct</from-outcome>
            <to-view-id>/UProduct.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/UStorage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editStorage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/UProduct.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/UProduct.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>404</from-outcome>
            <to-view-id>/404.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Manage</from-outcome>
            <to-view-id>/Manage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>UStorage</from-outcome>
            <to-view-id>/UStorage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/addStorage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/UStorage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editStorage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/UStorage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
